<%- include("partials/header.ejs",{title:"Dokter"}) %>
    <div class="content-wrapper">
      <div class="container-fluid">
        <!-- Breadcrumbs-->
        <ol class="breadcrumb">
          <li class="breadcrumb-item">
            <a href="#">Dashboard</a>
          </li>
          <li class="breadcrumb-item active">Dokter</li>
        </ol>
      </div>
      <div align="center">
        <a href="/dokters/add">ADD DOKTER</a>
      </div>
      <div class="card-body">
        <div class="table-responsive">
          <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <tr>
          <th>ID</th>
          <th>Name</th>
          <th>Alamat</th>
          <th>Contact</th>
          <th>Email</th>
          <th>Spesialis</th>
          <th>Action</th>
        </tr>
        <%

        let no = 1;

        rows.forEach(function(row) {

      %>
          <tr>
            <td>
              <%= no %>
            </td>
            <td>
              <%= row.name %>
            </td>
            <td>
              <%= row.alamat %>
            </td>
            <td>
              <%= row.contact %>
            </td>
            <td>
              <%= row.email %>
            </td>
            <td>
              <%= row.Kategori.kategori %>
            </td>
            <td> <a href="/dokters/edit/<%=row.id%>"> edit</a> <a href="/dokters/delete/<%=row.id%>">delete</a> </td>
          </tr>
      <%

      no++;

      })

      %>
      </table>
    </div>
    </div>
    </div>

<%- include partials/footer.ejs %>
